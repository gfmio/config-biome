{
  "$schema": "https://biomejs.dev/schemas/2.3.2/schema.json",
  "assist": {
    "actions": {
      "source": {
        "organizeImports": {
          "level": "on",
          "options": {
            "groups": [
              {
                "type": true
              },
              ":BLANK_LINE:",
              [":BUN:", ":NODE:"],
              ":BLANK_LINE:",
              ":PACKAGE:",
              ":BLANK_LINE:",
              ":ALIAS:",
              ":BLANK_LINE:",
              ":PATH:"
            ],
            "identifierOrder": "natural"
          }
        },
        "useSortedAttributes": {
          "level": "on",
          "options": {
            "sortOrder": "natural"
          }
        },
        "useSortedKeys": {
          "level": "on",
          "options": {
            "sortOrder": "natural"
          }
        },
        "useSortedProperties": {
          "level": "on",
          "options": {}
        }
      }
    },
    "enabled": true
  },
  "css": {
    "assist": {
      "enabled": true
    },
    "formatter": {
      "enabled": true,
      "indentStyle": "space",
      "indentWidth": 2,
      "lineEnding": "lf",
      "lineWidth": 120,
      "quoteStyle": "double"
    },
    "parser": {
      "allowWrongLineComments": false,
      "cssModules": true,
      "tailwindDirectives": true
    }
  },
  "files": {
    "ignoreUnknown": false,
    "includes": [
      "**/*.js",
      "**/*.jsx",
      "**/*.mjs",
      "**/*.cjs",
      "**/*.ts",
      "**/*.tsx",
      "**/*.mts",
      "**/*.cts",
      "**/*.json",
      "**/*.jsonc",
      "**/*.json5",
      "**/*.css",
      "**/*.html",
      "**/*.vue",
      "**/*.astro",
      "**/*.svelte",
      "**/*.graphql",
      "**/*.gql",
      "!!**/node_modules",
      "!!**/dist",
      "!!**/build",
      "!!**/out",
      "!!**/.next",
      "!!**/coverage",
      "!!**/.turbo",
      "!!**/.cache",
      "!!**/vendor",
      "!!**/*.min.js",
      "!!**/*.min.css",
      "!!**/*.bundle.{js,css}",
      "!!**/*.chunk.{js,css}"
    ],
    "maxSize": 1048576
  },
  "formatter": {
    "attributePosition": "auto",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "enabled": true,
    "expand": "auto",
    "formatWithErrors": false,
    "indentStyle": "space",
    "indentWidth": 2,
    "lineEnding": "lf",
    "lineWidth": 120,
    "useEditorconfig": true
  },
  "graphql": {
    "assist": {
      "enabled": true
    },
    "formatter": {
      "bracketSpacing": true,
      "enabled": true,
      "indentStyle": "space",
      "indentWidth": 2,
      "lineEnding": "lf",
      "lineWidth": 120,
      "quoteStyle": "double"
    },
    "linter": {
      "enabled": true
    }
  },
  "grit": {
    "assist": {
      "enabled": true
    },
    "formatter": {
      "enabled": true,
      "indentStyle": "space",
      "indentWidth": 2,
      "lineEnding": "lf",
      "lineWidth": 120
    },
    "linter": {
      "enabled": true
    }
  },
  "html": {
    "assist": {
      "enabled": true
    },
    "experimentalFullSupportEnabled": true,
    "formatter": {
      "attributePosition": "auto",
      "bracketSameLine": false,
      "enabled": true,
      "indentScriptAndStyle": false,
      "indentStyle": "space",
      "indentWidth": 2,
      "lineEnding": "lf",
      "lineWidth": 120,
      "selfCloseVoidElements": "never",
      "whitespaceSensitivity": "strict"
    }
  },
  "javascript": {
    "assist": {
      "enabled": true
    },
    "formatter": {
      "arrowParentheses": "always",
      "attributePosition": "auto",
      "bracketSameLine": false,
      "bracketSpacing": true,
      "enabled": true,
      "expand": "auto",
      "indentStyle": "space",
      "indentWidth": 2,
      "jsxQuoteStyle": "double",
      "lineEnding": "lf",
      "lineWidth": 120,
      "operatorLinebreak": "before",
      "quoteProperties": "asNeeded",
      "quoteStyle": "single",
      "semicolons": "always",
      "trailingCommas": "all"
    },
    "globals": [],
    "jsxRuntime": "transparent",
    "linter": {
      "enabled": true
    },
    "parser": {
      "gritMetavariables": true,
      "jsxEverywhere": false,
      "unsafeParameterDecoratorsEnabled": false
    }
  },
  "json": {
    "assist": {
      "enabled": true
    },
    "formatter": {
      "bracketSpacing": true,
      "enabled": true,
      "expand": "auto",
      "indentStyle": "space",
      "indentWidth": 2,
      "lineEnding": "lf",
      "lineWidth": 120,
      "trailingCommas": "none"
    },
    "linter": {
      "enabled": true
    },
    "parser": {
      "allowComments": false,
      "allowTrailingCommas": false
    }
  },
  "linter": {
    "enabled": true,
    "rules": {
